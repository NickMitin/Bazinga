{% set icons_file = {
					"default": "fa-file",
					"doc": "fa-file-word-o",
					"docx": "fa-file-word-o",
					"csv": "fa-file-excel-o",
					"xls": "fa-file-excel-o",
					"xlsx": "fa-file-excel-o",
					"jpg": "fa-file-image-o",
					"jpeg": "fa-file-image-o",
					"gif": "fa-file-image-o",
					"png": "fa-file-image-o",
					"svg": "fa-file-image-o",
					"avi": "fa-file-video-o",
					"mpg4": "fa-file-video-o",
					"mpg": "fa-file-video-o",
					"pdf": "fa-file-pdf-o",
					"php": "fa-file-code-o",
					"c": "fa-file-code-o",
					"js": "fa-file-code-o",
					"css": "fa-file-code-o",
					"mp3": "fa-file-audio-o",
					"wav": "fa-file-audio-o",
					"zip": "fa-file-archive-o",
					"rar": "fa-file-archive-o",
					"gz": "fa-file-archive-o",
					"tar": "fa-file-archive-o"
				}
%}
<i class="fa {% if file.file.getExtension() in icons_file|keys %}{{ icons_file[file.file.getExtension()] }}{% else %}{{ icons_file.default }}{% endif %}"></i>
